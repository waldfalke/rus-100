{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:local": "next build",
    "build:gh-pages": "GITHUB_PAGES=true next build",
    "build:gh-pages:win": "set GITHUB_PAGES=true&& next build",
    "start": "next start",
    "start:static": "npx serve out",
    "start:static:local": "npx serve out -p 3000",
    "lint": "next lint",
    "build:tokens": "node design-system/build.js",
    "build:tokens:watch": "node design-system/build.js --watch",
    "validate:backlog": "node rnd/distilled-methodology/scripts/validate-backlog.js master-backlog.md",
    "validate:traceability": "node rnd/distilled-methodology/scripts/validate-traceability.js traceability-matrix.csv",
    "find:orphaned": "node rnd/distilled-methodology/scripts/find-orphaned-tasks.js",
    "test:e2e": "playwright test --config=playwright-e2e.config.ts",
    "test:e2e:ui": "playwright test --config=playwright-e2e.config.ts --ui",
    "test:e2e:headed": "playwright test --config=playwright-e2e.config.ts --headed",
    "test:e2e:debug": "playwright test --config=playwright-e2e.config.ts --debug",
    "test:e2e:report": "playwright show-report playwright-report/e2e",
    "test:e2e:install": "playwright install",
    "test:bdd": "bddgen && playwright test --config=playwright.config.ts",
    "test:bdd:headed": "bddgen && playwright test --config=playwright.config.ts --headed",
    "test:bdd:ui": "bddgen && playwright test --config=playwright.config.ts --ui",
    "test:bdd:report": "playwright show-report",
    "test:tokens": "playwright test --config=playwright-e2e.config.ts tests/e2e/design-tokens.spec.ts",
    "test:visual": "playwright test tests/visual/",
    "test:visual:update": "playwright test tests/visual/ --update-snapshots",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:all": "npm run test:e2e && npm run test:bdd && npm run test:visual",
    "validate:tokens": "tsx scripts/validate-tokens.ts",
    "benchmark:tokens": "tsx scripts/validate-tokens.ts && node scripts/check-performance-regression.js summary",
    "audit:a11y": "lighthouse --chrome-flags=\"--headless\" --output=json --output-path=accessibility-report.json http://localhost:3001/demo",
    "build:demo": "npm run build",
    "update:tests": "node scripts/test-automation/update-tests-from-components.js",
    "sync:tokens": "node scripts/test-automation/sync-design-tokens.js",
    "prepare:tests": "npm run update:tests && npm run sync:tokens && bddgen",
    "verify:hardcoded": "tsx scripts/verify-hardcoded-values.ts",
    "verify:tokens-in-code": "tsx scripts/verify-code-uses-valid-tokens.ts",
    "verify:css-injection": "tsx scripts/verify-css-injection.ts",
    "verify:shadcn": "tsx scripts/verify-shadcn-token-requirements.ts"
  },
